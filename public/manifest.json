{
  "manifest_version": 3,
  "name": "DeWallet",
  "version": "1.0.0",
  "description": "Secure Ethereum wallet extension for Sepolia testnet",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "lovable-uploads/fcae4b49-2dcf-481d-b0d4-4df268c4d28f.png",
      "48": "lovable-uploads/fcae4b49-2dcf-481d-b0d4-4df268c4d28f.png",
      "128": "lovable-uploads/fcae4b49-2dcf-481d-b0d4-4df268c4d28f.png"
    }
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "icons": {
    "16": "lovable-uploads/fcae4b49-2dcf-481d-b0d4-4df268c4d28f.png",
    "48": "lovable-uploads/fcae4b49-2dcf-481d-b0d4-4df268c4d28f.png",
    "128": "lovable-uploads/fcae4b49-2dcf-481d-b0d4-4df268c4d28f.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  }
}